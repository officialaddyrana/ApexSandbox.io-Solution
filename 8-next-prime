public Integer nextPrime(Integer num) {
    //code here
    if(num<2) return 2;
    return num;
    while(!isPrime(++num));
    public boolean isPrime()
    {
        for (Integer i=2; i<num; i++) 
        {
        if (Math.mod(num, i) == 0) return false;
        }
         return true;
}
     }
   